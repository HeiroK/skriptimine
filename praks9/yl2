#!/bin/bash
# V채ljastab read ning t채rnid igas reas vastavalt kasutaja sisestusele
echo -n "Sisesta vajaminevad read: "
read rida
echo -n "Sisesta vajaminevate t채rnide arv reas: "
read tarnid
for (( i = 1 ; i<= $rida; i++ ))

	do
		for (( j = 1 ; j<= $tarnid; j++ ))
		do
		echo -n "*"
	done
echo ""
done
