#!/bin/bash
# V채ljastab t채rnid reas vastavalt kasutaja sisestusele kasvavas j채rjekorras
echo -n "Sisesta vajaminevad read: "
read rida
for (( i = 1 ; i<= $rida; i++ ))
	do
		for (( j = 1 ; j<= $rida; j++ ))
		do
		if [ $i -ge $j ]; then
		echo -n "*"
	else
	echo -n " "
	fi
	done
echo ""
done
